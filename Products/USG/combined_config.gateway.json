{
	"service": {
		"domain-name": "internal.mynetwork.com",
		"dns": {
			"forwarding": {
				"cache-size": 1000,
				"options": [
					"host-record=jarvis,172.30.31.1",
					"host-record=happy,172.30.31.2",
					"host-record=pepper,172.30.31.3"
				]
			}
		},
		"upnp2": {
			"listen-on": [
				"eth1"
			],
			"nat-pmp": "enable",
			"secure-mode": "enable",
			"wan": "eth0"
		}
	}
}
